@model Elmah.Web.Models.HomeViewData

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<div class="row">
    <div class="col-sm-12">
        <h1>Application Health</h1>
    </div>
</div>
<div class="bs-docs-grid">
    <div class="row">
        @foreach (var application in Model.Applications)
        {
            <div class="col-md-4 col-sm-4">
                <div class="panel panel-default panel-application bg-success">
                    <div class="panel-body">
                        <div class="value">Application State</div>
                        <div class="title">
                            <i class="fa fa-usd"></i>
                            <span class="m-left-xs">@application.Name</span>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>
